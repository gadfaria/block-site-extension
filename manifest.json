{
  "manifest_version": 3,
  "name": "Block Sites Extensions",
  "description": "An extension to block sites and help you stay focused",
  "version": "1.0",
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
